        <form string="Contracts">
          <sheet>
            <group>
                <group>
                    <field name="name"/>
                    <field name="partner_id"/>
                </group>
                <group>
                    <field name="contact"/>
                    <field name="job_title"/>
                    <field name="health_safety" widget="many2many_tags"/>
                    <field name="premesis_type"/>
                </group>
              </group>
                          <notebook>
              <page string="System Information">
                  <group>
                        <group>
                            <field name="system_type"/>
                            <field name="build_standard"/>
                            <field name="system_description"/>
                            <field name="primary_signal_type"/>
                        </group>
                        <group>
                            <field name="category_type"/>
                            <field name="grade"/>
                            <field name="system_condition"/>
                            <field name="status"/>
                            <field name="contract_type"/>
                            <field name="premesis_type"/>
                        </group>
                    </group>

              </page>
              <page string="Site Details">
                  <group>
                        <group>
                            <field name="site_address"/>
                            <field name="site_email"/>
                        </group>
                        <group>
                            <field name="site_mobile"/>
                            <field name="site_phone"/>
                        </group>
                    </group>
              </page>
              <page string="PMV Service Setup">
                   <group>
                     <field name="pmv_ids"/>

                    </group>
              </page>
              <page string="Remote Monitoring">
                   <group>
                       <field name="remote_ids"/>
                    </group>
              </page>
               <page string="Finance">
                   <group>
                        <group>
                            <field name="maint_from"/>
                            <field name="maint_to"/>
                            <field name="account_number"/>
                            <field name="call_out_account"/>
                            <field name="inv_code"/>
                            <field name="paid_by"/>
                            <field name="mon_rate"/>
                            <field name="key_rate"/>
                            <field name="guard_rate"/>
                            <field name="call_out_includes"/>
                        </group>
                        <group>
                            <field name="travel_time"/>
                            <field name="email_invoice"/>
                            <field name="po_req_callout"/>
                            <field name="callout_po"/>
                            <field name="callout_po_expires"/>
                            <field name="po_req_main"/>
                            <field name="maint_po"/>
                            <field name="maint_po_expires"/>
                            <field name="account_number"/>
                            <field name="call_out_account"/>
                        </group>
                    </group>
              </page>
                <page string="URNs">
                   <group>
                       <field name="urn_ids"/>

                    </group>
              </page>
                <page string="Private Notes}">
                   <group>
                       <field name="private_notes"/>

                    </group>
              </page>
                <page string="Service Engineer Notes">
                    <group>
                        <field name="engineer_notes"/>
                    </group>
                   <group>
                        <group>
                            <field name="service_reminder_1"/>
                            <field name="service_reminder_2"/>
                            <field name="service_reminder_3"/>
                            <field name="service_reminder_4"/>
                        </group>
                        <group>
                            <field name="service_reminder_date_1"/>
                            <field name="service_reminder_date_2"/>
                            <field name="service_reminder_date_3"/>
                            <field name="service_reminder_date_4"/>
                        </group>
                    </group>
              </page>
                <page string="Loss Reason">
                   <group>

                        <group>
                            <field name="loss_date"/>
                            <field name="loss_effective_date"/>
                            <field name="loss_reason"/>

                        </group>
                        <group>
                            <field name="details_removed"/>
                            <field name="lost_serivce_pa"/>
                            <field name="destroy_docs"/>
                        </group>
                    </group>
              </page>
              <page string="Misc">

                   <group>

                        <group>
                            <field name="rel_contract"/>
                        </group>
                        <group>
                            <field name="rel_quote"/>
                        </group>
                    </group>
              </page>
            </notebook>

          </sheet>
          <div class="oe_chatter">
                <field name="message_follower_ids" widget="mail_followers"/>
                <field name="message_ids" widget="mail_thread"/>
          </div>
        </form>
      </field>
    </record>


    <record id="contract_search" model="ir.ui.view">
       <field name="name">psg.contract.search</field>
       <field name="model">psg.contract</field>
        <field name="arch" type="xml">
            <search string="Search Contracts">
                <field name="name" string="Board Name" domain="[()]"/>

                <group expand="0" string="Group By">
                        <filter string="Contract" name="name" context="{'group_by': 'name'}"/>

                </group>

        </search>
       </field>
    </record>

